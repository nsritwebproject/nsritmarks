        _posturl = "../jqmethods.aspx/SaveCoursesCertificates";
        _postdata = "{'courseid':" + $("#ddlcourse").val() + ",'arrcertificateid':" + JSON.stringify(_arrcertificateid) + "}";
        executeAjaxMethod(_posturl, _postdata, "CCS");
    }
}
